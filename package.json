{
    "name": "intelligence-change-case",
    "displayName": "change-case",
    "description": "Intelligence change case.",
    "version": "1.3.0",
    "repository": "https://github.com/k23450987/vscode-intelligence-change-case",
    "publisher": "k23450987",
    "engines": {
        "vscode": "^1.74.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [],
    "main": "./out/extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.changeCase",
                "title": "Intelligence change case."
            },
            {
                "command": "extension.changeLowerUpperCase",
                "title": "Intelligence change lower upper case."
            }
        ],
        "keybindings": [
            {
                "key": "Ctrl+U",
                "command": "extension.changeCase",
                "when": "editorTextFocus"
            },
            {
                "key": "Ctrl+Shift+U",
                "command": "extension.changeLowerUpperCase",
                "when": "editorTextFocus"
            }
        ],
        "configuration": {
            "title": "Intelligence change case settings.",
            "properties": {
                "change-case.py": {
                    "type": "array",
                    "default": [
                        "snake_case",
                        "PascalCase",
                        "CONST_CASE"
                    ]
                },
                "change-case.go": {
                    "type": "array",
                    "default": [
                        "camelCase",
                        "PascalCase",
                        "CONST_CASE"
                    ]
                },
                "change-case.js": {
                    "type": "array",
                    "default": [
                        "camelCase",
                        "PascalCase",
                        "CONST_CASE"
                    ]
                },
                "change-case.ts": {
                    "type": "array",
                    "default": [
                        "camelCase",
                        "PascalCase",
                        "CONST_CASE"
                    ]
                },
                "change-case.java": {
                    "type": "array",
                    "default": [
                        "camelCase",
                        "PascalCase",
                        "CONST_CASE"
                    ]
                },
                "change-case.cs": {
                    "type": "array",
                    "default": [
                        "camelCase",
                        "PascalCase",
                        "CONST_CASE"
                    ]
                },
                "change-case.cpp": {
                    "type": "array",
                    "default": [
                        "camelCase",
                        "PascalCase",
                        "CONST_CASE"
                    ]
                },
                "change-case.c": {
                    "type": "array",
                    "default": [
                        "snake_case",
                        "camelCase",
                        "PascalCase",
                        "CONST_CASE"
                    ]
                },
                "change-case.css": {
                    "type": "array",
                    "default": [
                        "snake_case",
                        "param-case"
                    ]
                },
                "change-case.default": {
                    "type": "array",
                    "default": [
                        "camelCase",
                        "CONST_CASE"
                    ]
                }
            }
        }
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "lint": "eslint \"src/**/*.ts\"",
        "watch": "tsc -watch -p ./"
    },
    "devDependencies": {
        "@types/vscode": "^1.74.0",
        "typescript": "^5.0.4"
    },
    "dependencies": {
        "change-case": "^4.1.2",
        "vsce": "^2.15.0"
    }
}